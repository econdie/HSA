<Application x:Class="HSA.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HSA.Views"
             xmlns:local2="clr-namespace:HSA.ViewModels"
             Startup="Application_Startup"
             ShutdownMode="OnExplicitShutdown">
    <Application.Resources>
        <DataTemplate DataType="{x:Type local2:MainWindowViewModel}">
            <local:MainWindowView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type local2:ArenaRunEditViewModel}">
            <local:ArenaRunEditView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type local2:ArenaRunAddViewModel}">
            <local:ArenaRunAddView />
        </DataTemplate>
    </Application.Resources>
</Application>
